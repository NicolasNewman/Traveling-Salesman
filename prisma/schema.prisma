//   guildId String
//   number  String
// }

model Guild {
  name              String?
  guildId           String  @unique
  prefix            String  @default("/")
  // items
  weiId             String?
  sianId            String?
  kaysarrId         String?
  madnickId         String?
  seriaId           String?
  mokamokaId        String?
  rapportId         String?
  // channels
  readChannelId     String  @default("")
  writeChannelId    String  @default("")
  // flags
  flags             Int     @default(0) @db.SmallInt
  // reactions
  reactionMessageId String  @default("")
}
